{
  "semi": false,
  "tabWidth": 4,
  "printWidth": 100,
  "singleQuote": true,
  "trailing-comma": [
    true,
    {
      "singleline": "never",
      "multiline": {
        "objects": "always",
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      }
    }
  ],
  "bracketSpacing": true,
  "bracketSameLine": false
}
